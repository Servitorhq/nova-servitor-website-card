{
    "name": "servitorhq/nova-servitor-monitor-card",
    "description": "Simple card for Laravel Nova to show your website/monitor stats from Servitor.io",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Servitor\\NovaServitorMonitorCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Servitor\\NovaServitorMonitorCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
